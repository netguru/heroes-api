{
  "name": "new-recru-api",
  "version": "1.0.0",
  "description": "",
  "main": "commintlint.conf.js",
  "dependencies": {
    "body-parser-graphql": "^1.1.0",
    "express": "^4.17.1",
    "gql2ts": "^1.10.1",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.17.4",
    "prisma-client-lib": "^1.34.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/express": "^4.16.1",
    "@types/node": "^12.0.3",
    "husky": "^2.3.0",
    "ts-node": "^8.2.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "prepare": "prisma deploy && npm run start:rest",
    "start:graphql": "ts-node GRAPHQL_index.ts",
    "start:rest": "ts-node REST_index.ts",
    "dev:rest": "ts-node-dev --no-notify --respawn --transpileOnly ./REST_index.ts",
    "dev:graphql": "ts-node-dev --no-notify --respawn --transpileOnly ./GRAPHQL_index.ts",
    "deploy": "npm run gst && prisma deploy",
    "gst": "ts-node gen-schema.ts"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
